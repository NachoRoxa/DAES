@model DAES.Model.SistemaIntegrado.Organizacion

<table class="table table-hover table-striped datatable">
    Disolucion Asociacion Consumidores
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().NumeroOficio)
            </th>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().FechaOficio)
            </th>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().FechaAsambleaSocios)
            </th>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().FechaEscrituraPublica)
            </th>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().FechaPublicacionDiarioOficial)
            </th>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().NombreNotaria)
            </th>
            <th>
                @Html.DisplayNameFor(model => Model.DisolucionAsociacions.FirstOrDefault().DatosNotario)
            </th>
        </tr>
    </thead>
    <tbody>
        @for (var i = 0; i < Model.DisolucionAsociacions.Count(); i++)
        {
            <tr>
                @Html.EditorFor(m => m.DisolucionAsociacions[i])
                <td>
                    @Ajax.ActionLink("Eliminar", "DisolucionDelete", null, new { Model.DisolucionAsociacions.ToArray()[i].DisolucionAsociacionId }, new AjaxOptions { UpdateTargetId = "disolucion" })
                </td>
            </tr>
        }
    </tbody>
</table>
